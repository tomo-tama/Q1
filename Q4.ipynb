   "source": [
    "def gcd(a, b):\n",
    "  while b > 0:\n",
    "    a, b = b, a % b\n",
    "  return a\n",
    "\n",
    "def lcm(a, b):\n",
    "  return a * b / gcd(a, b)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    src = range(2, 21)\n",
    "    c=1  \n",
    "    for i in src:\n",
    "        d = lcm(c, i)\n",
    "        c=d\n",
    "    print(c)"
   ]
  }
